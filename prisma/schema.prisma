datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Color {
  id      String  @id
  p1      String
  p2      String
  p3      String
  p4      String
  red     Boolean
  blue    Boolean
  yellow  Boolean
  green   Boolean
}